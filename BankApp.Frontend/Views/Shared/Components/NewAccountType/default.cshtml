@model BankApp.Enteties.DataTransferObjects.AccountTypeDTO

<div class="container">

    <form asp-action="NewAccountType" asp-controller="Admin">

        <div class="form-floating mb-3">
            <input type="text" asp-for="TypeName" class="form-control" />
            <label asp-for="TypeName"></label>
        </div>
        <div class="form-floating mb-3">
            <input type="text" asp-for="Description" class="form-control" />
            <label asp-for="Description"></label>
        </div>

        <button type="submit" class="btn btn-primary">Skapa AccountTyp</button>
    </form>
</div>
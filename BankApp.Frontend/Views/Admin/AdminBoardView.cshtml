<div class="container-fluid">
    <div class="row">

        <div class="col border-right">
            @*Customer list*@
            @await Html.PartialAsync("_CustomersPartial", ViewData["Customers"])
        </div>
        <div class="col border-right">
            @*New Customer Acc*@
            <partial name="_RegCustomerPartial" />
        </div>
        <div class="col border-right">
            @await Component.InvokeAsync("RegAccount")
        </div>
        <div class="col border-right">
            @*Customer login*@
            @await Component.InvokeAsync("AddCustomerLogin")
        </div>
        <div class="col border-right">
            @*New accounttype*@
            @await Component.InvokeAsync("NewAccountType")
        </div>
    </div>
</div>